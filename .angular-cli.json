{
  "project": {
    "version": "1.0.0-beta.24",
    "name": "angular2-express-starter"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/chart.js/dist/Chart.bundle.js",
        "../node_modules/flexslider/jquery.flexslider.js",
        "lib/html5shiv.js",
        "lib/imagesloaded.pkgd.min.js",
        "lib/isotope.pkgd.min.js",
        "lib/jquery-migrate-1.2.1.min.js",
        "lib/jquery-ui-1.10.2.custom.js",
        "lib/jquery.cookie.js",
        "lib/jquery.easing.min.js",
        "lib/jquery.elevatezoom.js",
        "lib/la_boutique.js",
        "lib/megamenu.js"
      ],
      "environmentSource": "environments/environment.ts",
      "_environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [
    "../node_modules/font-awesome/fonts/*.+(otf|eot|svg|ttf|woff|woff2)"
  ],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    }
  }
}
